{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
    { "include": "**/*.graphql", "exclude": "**/omitted.graphql", "watchAssets": true },
    { "include": "../config/*.yml", "outDir": "./dist/config"},
    { "include": "../config/*.yaml", "outDir": "./dist/config"}
    ]
  }
}
